{
    "name": "inly/duster-presets",
    "description": "Inly's presets for the duster package",
    "keywords": [
        "inly",
        "laravel",
        "duster-presets"
    ],
    "homepage": "https://github.com/inly/duster-presets",
    "license": "MIT",
    "authors": [
        {
            "name": "Inly Technology AB",
            "email": "hello@inly.se",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Inly\\DusterPresets\\": "src/",
            "Inly\\DusterPresets\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inly\\DusterPresets\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Inly\\DusterPresets\\DusterPresetsServiceProvider"
            ],
            "aliases": {
                "DusterPresets": "Inly\\DusterPresets\\Facades\\DusterPresets"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}