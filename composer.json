{
    "name": "inly/duster-presets",
    "description": "Inly's presets for the duster package",
    "keywords": [
        "inly",
        "laravel",
        "duster-presets"
    ],
    "homepage": "https://github.com/inly/duster-presets",
    "license": "MIT",
    "authors": [
        {
            "name": "Inly Technology AB",
            "email": "hello@inly.se",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Inly\\DusterPresets\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Inly\\DusterPresets\\DusterPresetsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}